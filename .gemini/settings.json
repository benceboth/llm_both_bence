{
  "mcpServers": {
    "Framelink Figma MCP": {
      "command": "npx",
      "args": [
        "-y",
        "figma-developer-mcp",
        "--figma-api-key=${FIGMA_API_KEY}",
        "--stdio"
      ],
      "type": "stdio",
      "trust": false
    },
    "GitLab": {
      "command": "npx",
      "args": ["-y", "gitlab-mcp", "--stdio"],
      "type": "stdio",
      "trust": false
    }
  }
}
